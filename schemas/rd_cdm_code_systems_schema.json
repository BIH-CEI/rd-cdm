{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "RD CDM Code Systems Schema",
    "type": "object",
    "properties": {
      "CodeSystems": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "codeSystemName": {
              "type": "string",
              "description": "The name of the code system."
            },
            "namespace_prefix": {
              "type": "string",
              "description": "The namespace prefix of the code system."
            },
            "version": {
              "type": "string",
              "description": "The version of the code system."
            },
            "url": {
              "type": "string",
              "description": "The URL of the code system."
            },
            "synonyms": {
              "type": "array",
              "items": {
                "type": "string"
              },
              "description": "Optional synonyms for the code system."
            }
          },
          "required": ["codeSystemName", "namespace_prefix", "version", "url"]
        }
      }
    },
    "required": ["CodeSystems"]
  }
  